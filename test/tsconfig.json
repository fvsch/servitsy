{
	"include": ["./*.ts"],
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ES2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"types": ["node"],
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"isolatedModules": true,
		"erasableSyntaxOnly": true,
		"noEmit": true
	}
}
